{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "pokemon-type/wp-pokemon",
	"version": "0.1.0",
	"title": "Pokemon Type",
	"category": "embed",
	"icon": "games",
	"description": "Pokemon Block Description",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"selectedType": {
			"type": "string",
			"default": ""
		},
		"pokemons": {
			"type": "array",
			"default": []
		},
		"backgroundColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"textColor": {
			"type": "string",
			"default": "#000000"
		},
		"fontSize": {
			"type": "number",
			"default": 16
		},
		"itemBackground": {
			"type": "string",
			"default": "#f5f5f5"
		},
		"itemTextColor": {
			"type": "string",
			"default": "#333333"
		},
		"customClassName": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "wp-pokemon",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
